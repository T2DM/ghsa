{% if page.header %}
  <div class="row">
    <div class="large-12 columns">
      {{ page.header }}
    </div>
  </div>
{% endif %}

<div class="row">
  {% if show_account_info %}
    <div class="{{ site_slogan ? 'large-6' : 'large-4 columns large-offset-8' }} columns hide-for-small">
      <p>
        {% if logged_in %}
          <a href="{{ path('user.page') }}">{{ 'My Account'|t }}</a>
          <a href="{{ path('user.logout') }}">{{ 'Logout'|t }}</a>
        {% else %}
          <a href="{{ path('user.login') }}">{{ 'Login'|t }}</a>
          <a href="{{ path('user.register') }}">{{ 'Sign Up'|t }}</a>
        {% endif %}
      </p>
    </div>
  {% endif %}
  {% if site_slogan %}
    <div class="large-6 columns hide-for-small">
      {{ site_slogan }}
    </div>
  {% endif %}
  {% if show_account_info %}
    <div class="show-for-small">
      <div class="six mobile-two columns">
        <p>
          <a href="{{ path('user.login') }}" class="radius button">{{ 'Login'|t }}</a>
        </p>
      </div>
      <div class="six mobile-two columns">
        <p>
          <a href="{{ path('user.register') }}" class="radius success button">{{ 'Sign Up'|t }}</a>
        </p>
      </div>
    </div>
  {% endif %}
</div>

{% if messages and not zurb_foundation_messages_modal %}
  <div class="l-messages row">
    <div class="large-12 columns">
      {{ messages }}
    </div>
  </div>
{% endif %}

{% if page.help %}
<div class="l-help row">
  <div class="large-12 columns">
    {{ page.help }}
  </div>
</div>
{% endif %}

<div class="row">
  <main id="main" class="{{ main_grid }} columns" role="main">
    {% if page.highlighted %}
      {% spaceless %}
        {{ page.highlighted }}
      {% endspaceless %}
    {% endif %}
    <a id="main-content"></a>
    {% if breadcrumb %} {{ breadcrumb }} {% endif %}
    <section>
      {{ page.content }}
    </section>
  </main>

</div>

<!-- START: footer : contains footer elements -->
<footer>

  {% if page.footer_row_1_title or page.footer_row_1_column_1 or page.footer_row_1_column_2 or page.footer_row_1_column_3 %}
  	<!-- START: section.footer_contact_information -->
  	<section class="footer_contact_information">

  		<!-- START: .row -->
  		<div class="row footer_contact_information_data">

  			<!-- START: columns -->
  			<div class="columns small-10 small-offset-1 end column_no_padding_both">

  				<div class="inner_column region_contact_block_title">
  					{% if page.footer_row_1_title %}
  						{{ page.footer_row_1_title }}
  					{% endif %}
  				</div>
  				<div class="inner_column">
  					{% if page.footer_row_1_column_1 %}
  						{{ page.footer_row_1_column_1 }}
  					{% endif %}
  				</div>
  				<div class="inner_column">
  					{% if page.footer_row_1_column_2 %}
  						{{ page.footer_row_1_column_2 }}
  					{% endif %}
  				</div>
  				<div class="inner_column">
  					{% if page.footer_row_1_column_3 %}
  						{{ page.footer_row_1_column_3 }}
  					{% endif %}
  				</div>

  			</div>
  			<!-- END: columns -->

  		</div>
  		<!-- END: .row -->

  	</section>
  	<!-- END: section.footer_contact_information -->
  {% endif %}

  <!-- START: section.footer_primary_menu_container -->
	<section class="footer_primary_menu_container row">

		<!-- START: div.footer_primary_menu -->
		<nav class="footer_primary_menu columns small-10 small-offset-1 end column_no_padding_both" id="js_footer_primary_menu">

			{% if page.footer_row_2_menu %}
				{{ page.footer_row_2_menu }}
			{% endif %}

		</nav>
		<!-- END: div.footer_primary_menu -->

	</section>
	<!-- END: section.footer_primary_menu_container -->

	<!-- START: section.footer_copyright -->
	<section class="footer_copyright row">

		<div class="columns small-10 small-offset-1 end column_no_padding_both">
			{% if page.footer_row_2_copyright %}
				{{ page.footer_row_2_copyright }}
			{% endif %}
		</div>

	</section>
	<!-- END: section.footer_copyright -->
</footer>
