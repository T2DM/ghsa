			<div class="header_mobile_menu_toggle" id="js_header_mobile_menu_toggle">Mobile Menu Toggle</div>
		</div>

		<!-- START: div.header_logo -->
		<div class="header_logo_container inner_column column_no_padding_both">
			<div class="header_logo">
        {% if linked_logo %}
          {{ linked_logo }}
        {% endif %}
				<!-- <div class="header_logo_bug"><img src="/themes/ghsa/images/logo-ghsa-1.png" alt="GHSA: Governors Highway Safety Association" /></div> -->
			</div>
			<div class="header_logo_logotype">
				<div class="header_logo_logotype_organization">Governors Highway Safety Association</div>
				<div class="header_logo_logotype_tagline">The Statesâ€™ Voice on Highway Safety</div>
			</div>
		</div>
		<!-- END: div.header_logo -->

		<!-- START: div.header_primary_menu_container -->
		<div id="js_header_primary_menu_container" class="header_primary_menu_container inner_column column_no_padding_both">

			<nav class="header_primary_menu" id="js_primary_menu">

				{% if page.header_row_1_menu_primary %}
					{{ page.header_row_1_menu_primary }}
				{% endif %}

			</nav>


		</div>
		<!-- END: div.header_primary_menu_container -->

		<li class="secondary_menu_member_login" title="Member Login"><a href="http://my.ghsa.org/">Member Login</a></li>

		<!-- START: div.header_secondary_menu_container -->
